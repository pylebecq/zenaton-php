{
    "name": "zenaton/zenaton-php",
    "type": "library",
    "description": "Zenaton PHP library",
    "homepage": "https://zenaton.com",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.6.0",
        "vlucas/phpdotenv": "^2.4",
        "nategood/httpful": "^0.2",
        "jeremeamia/superclosure": "^2.0",
        "cakephp/chronos": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4" : {
            "Zenaton\\Exceptions\\": "src/Zenaton/Exceptions/",
            "Zenaton\\": "src/Zenaton/v1/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zenaton\\Test\\": "tests/Zenaton/Test/",
            "Zenaton\\": "tests/Zenaton/v1/"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    }
}
